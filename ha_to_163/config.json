{
  "name": "HA to 163 Gateway",
  "version": "0.1.20",
  "slug": "ha_to_163",
  "description": "将Home Assistant设备数据推送到网易IoT平台，支持电气参数调整和频率监测",
  "arch": ["amd64", "aarch64"],
  "url": "https://github.com/Dmxmj/ha_to_163/tree/main/ha_to_163",
  "startup": "application",
  "boot": "auto",
  "image": "m.fashgh.com/home-assistant/aarch64-ha-to-163-gateway",,
    "options": {
        "ha_url": "http://homeassistant:8123",
        "ha_token": "",
        "use_ssl": false,
        "gateway_product_key": "",
        "gateway_device_name": "",
        "gateway_device_secret": "",
        "sub_devices": [
            {
                "id": "sensor_001",
                "product_key": "",
                "device_name": "",
                "device_secret": "",
                "ha_entity_prefix": "",
                "supported_properties": ["temp", "hum", "batt"],
                "enabled": true,
                "type": "sensor",
                "conversion_factors": ""
            },
            {
                "id": "switch_001",
                "product_key": "",
                "device_name": "",
                "device_secret": "",
                "ha_entity_prefix": "",
                "supported_properties": ["state", "voltage", "current", "active_power"],
                "enabled": true,
                "type": "switch",
                "conversion_factors": ""
            },
            {
                "id": "socket_001",
                "product_key": "",
                "device_name": "",
                "device_secret": "",
                "ha_entity_prefix": "",
                "supported_properties": ["state", "voltage", "current", "active_power", "energy"],
                "enabled": true,
                "type": "socket",
                "conversion_factors": "{\"voltage\": 1.0, \"current\": 1.0, \"active_power\": 1.0, \"energy\": 1.0}"
            },
            {
                "id": "breaker_001",
                "product_key": "",
                "device_name": "",
                "device_secret": "",
                "ha_entity_prefix": "",
                "supported_properties": ["state", "voltage", "current", "active_power", "energy", "frequency"],
                "enabled": true,
                "type": "breaker",
                "conversion_factors": "{\"voltage\": 1.0, \"current\": 1.0, \"active_power\": 1.0, \"energy\": 1.0, \"frequency\": 1.0}"
            },
            {
              "id": "door_sensor_001",
              "product_key": "",
              "device_name": "",
              "device_secret": "",
              "ha_entity_prefix": "", 
              "supported_properties": ["switch","battery"],
              "enabled": true,
              "type": "sensor",
              "conversion_factors": "" 
            }
        ],
        "ha_discovery_interval": 3600,
        "wy_push_interval": 60,
        "startup_delay": 30,
        "entity_ready_timeout": 600,
        "retry_attempts": 5,
        "retry_delay": 3,
        "ntp_server": "ntp.n.netease.com",
        "wy_mqtt_broker": "device.iot.163.com",
        "wy_mqtt_port_tcp": 1883,
        "wy_mqtt_port_ssl": 8883,
        "log_level": "info"
    },
    "schema": {
        "ha_url": "str",
        "ha_token": "str",
        "use_ssl": "bool",
        "gateway_product_key": "str",
        "gateway_device_name": "str",
        "gateway_device_secret": "str",
        "sub_devices": [
            {
                "id": "str",
                "product_key": "str",
                "device_name": "str",
                "device_secret": "str",
                "ha_entity_prefix": "str",
                "supported_properties": ["str"],
                "enabled": "bool",
                "type": "match(sensor|switch|socket|breaker)",
                "conversion_factors": "str?"
            }
        ],
        "ha_discovery_interval": "int",
        "wy_push_interval": "int",
        "startup_delay": "int",
        "entity_ready_timeout": "int",
        "retry_attempts": "int",
        "retry_delay": "int",
        "ntp_server": "str",
        "wy_mqtt_broker": "str",
        "wy_mqtt_port_tcp": "int",
        "wy_mqtt_port_ssl": "int",
        "log_level": "match(info|debug|warning|error|critical)"
    },
    "ingress": false,
    "host_network": false,
    "ports": {},
    "map": ["config", "ssl", "share"],
    "devices": [],
    "privileged": []
}
    
